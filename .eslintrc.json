{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["xo"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "array-bracket-spacing": "off",
    "new-cap": "off",
    "no-return-assign": "off",
    "no-undef": "off",
    "no-unused-vars": "off",
    "no-useless-constructor": "off",
    "object-curly-spacing": "off",
    "camelcase": "warn"
  }
}
