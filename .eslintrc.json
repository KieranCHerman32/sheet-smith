{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "xo"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-useless-constructor": "off",
    "new-cap": "off",
    "no-undef": "off",
    "no-return-assign": "off",
    "no-unused-vars": "off",
    "object-curly-spacing": "off",
    "array-bracket-spacing": "off"
  }
}
